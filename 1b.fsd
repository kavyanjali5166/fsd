// App.js 
import React from 'react'; 
function Child({ message }) { 
 return <h3>Message from Parent: {message}</h3>; 
} 
function App() { 
 const data = "Hello from Parent Component!"; 
 return ( 
 <div> 
 <h2>React Data Sharing Example</h2> 
 <Child message={data} /> 
 </div> 
 ); 
}
export default App; 

